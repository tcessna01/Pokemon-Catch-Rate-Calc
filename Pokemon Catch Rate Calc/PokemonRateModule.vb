    Public Sub getArray(ByVal cboxLength As Integer, ByVal cboxIndex As Integer, ByVal limitedIndex As Integer, ByRef txtBoxCatchRate As Object)

        If limitedIndex = 0 Then
            For index = 0 To cboxLength
                If index = cboxIndex Then
                    txtBoxCatchRate.Text = pokeRateAll(index)
                End If
            Next
        ElseIf limitedIndex = 1 Then
            For index = 0 To cboxLength
                If index = cboxIndex Then
                    txtBoxCatchRate.Text = pokeRateGen1(index)
                End If
            Next
        ElseIf limitedIndex = 2 Then
            For index = 0 To cboxLength
                If index = cboxIndex Then
                    txtBoxCatchRate.Text = pokeRateGen2(index)
                End If
            Next
        ElseIf limitedIndex = 3 Then
            For index = 0 To cboxLength
                If index = cboxIndex Then
                    txtBoxCatchRate.Text = pokeRateGen3(index)
                End If
            Next
        ElseIf limitedIndex = 4 Then
            For index = 0 To cboxLength
                If index = cboxIndex Then
                    txtBoxCatchRate.Text = pokeRateGen4(index)
                End If
            Next
        ElseIf limitedIndex = 5 Then
            For index = 0 To cboxLength
                If index = cboxIndex Then
                    txtBoxCatchRate.Text = pokeRateGen5(index)
                End If
            Next
        ElseIf limitedIndex = 6 Then
            For index = 0 To cboxLength
                If index = cboxIndex Then
                    txtBoxCatchRate.Text = pokeRateGen6(index)
                End If
            Next
        End If

    End Sub

    Public Sub setLimitedPokemon(ByVal cboxIndex As Integer, ByRef cboxSelectPoke As Object)

        cboxSelectPoke.Items.Clear()

        If cboxIndex = 0 Then
            For index = 0 To pokesGenAll.Length - 1
                cboxSelectPoke.Items.Add(pokesGenAll(index))

            Next
        ElseIf cboxIndex = 1 Then
            For index = 0 To pokesGen1.Length - 1
                cboxSelectPoke.Items.Add(pokesGen1(index))
            Next
        ElseIf cboxIndex = 2 Then
            For index = 0 To pokesGen2.Length - 1
                cboxSelectPoke.Items.Add(pokesGen2(index))
            Next
        ElseIf cboxIndex = 3 Then
            For index = 0 To pokesGen3.Length - 1
                cboxSelectPoke.Items.Add(pokesGen3(index))
            Next
        ElseIf cboxIndex = 4 Then
            For index = 0 To pokesGen4.Length - 1
                cboxSelectPoke.Items.Add(pokesGen4(index))
            Next
        ElseIf cboxIndex = 5 Then
            For index = 0 To pokesGen5.Length - 1
                cboxSelectPoke.Items.Add(pokesGen5(index))
            Next
        ElseIf cboxIndex = 6 Then
            For index = 0 To pokesGen6.Length - 1
                cboxSelectPoke.Items.Add(pokesGen6(index))
            Next
        End If

        cboxSelectPoke.SelectedIndex = 0
    End Sub
